<style rel="stylesheet">
{{ base_font | font_face: font_display: 'swap' }}
{%- if base_font.weight != '500' -%}
  {%- assign base_font_500 = base_font | font_modify: 'weight', '500' -%}
  {{ base_font_500 | font_face: font_display: 'swap' }}
{%- endif -%}
{%- if base_font != base_font_bold -%}
  {{ base_font_bold | font_face: font_display: 'swap' }}
{%- endif -%}
{%- if base_font != base_font_bolder and base_font_bolder != base_font_bold -%}
/* bolder */
  {{ base_font_bolder | font_face: font_display: 'swap' }}
{%- endif -%}
{%- if base_font != base_font_italic -%}
  {{ base_font_italic | font_face: font_display: 'swap' }}
{%- endif -%}
{%- if base_font_bold_italic != base_font_bold  and base_font_bold_italic != base_font and base_font_bold_italic != base_font_italic -%}
  {{ base_font_bold_italic | font_face: font_display: 'swap' }}
{%- endif -%}
{{ heading_font | font_face: font_display: 'swap' }}
{%- assign heading_font_bold = heading_font | font_modify: 'weight', 'bolder' -%}
{%- if heading_font != heading_font_bold -%}
  {{ heading_font_bold | font_face: font_display: 'swap' }}
{%- endif -%}
{%- render 'preload-styles.css' with left_holder: left_holder, right_holder: right_holder -%}
{%- render 'preload-search-full.css' with left_holder: left_holder, right_holder: right_holder -%}
{%- if settings.lazyload_effect -%}{%- render 'preload-lazy-load.css' with left_holder: left_holder, right_holder: right_holder -%}{%- endif -%}
{%- if template.name == 'index' -%}
	{%- render 'preload-home-page.css' with left_holder: left_holder, right_holder: right_holder -%}
{%- else -%}
	{%- render 'preload-breadcrumbs.css' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
{%- if settings.promo_enable -%}
{%- unless enable_rtl -%}
.promo__item:last-child {padding-right: 0;}
{%- else -%}
.promo__item:first-child {padding-right: 0;}
{%- endunless -%}
{%- render 'preload-promotion.css' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
  {%- render 'preload-product-single-general.css' with left_holder: left_holder, right_holder: right_holder -%}
{%- if template.name == 'product' -%}
	{%- render 'preload-product-single.css' with left_holder: left_holder, right_holder: right_holder -%}
	{%- render 'preload-product-single-fre-bought.css' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
{%- if template.name == 'collection' or template.name == 'search' or template.name == 'blog' -%}{%- render 'preload-grid-list.css' with left_holder: left_holder, right_holder: right_holder -%}{%- endif -%}
{%- if template.name == 'collection' or template.name == 'search' -%}
	{%- render 'preload-collection-page.css' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
{%- if template.name == 'search' -%}
  {%- render 'preload-search-page.css' with left_holder: left_holder, right_holder: right_holder -%}
{%- endif -%}
{%- if template.name == 'blog' or template.name == 'article' -%}{%- render 'preload-blog-pages.css' with left_holder: left_holder, right_holder: right_holder -%}{%- endif -%}
{%- if settings.thumbnail_ratio != 'auto' -%}.pg__image{padding-top:{{ settings.thumbnail_ratio }}%;}{%- endif -%}
</style>
<style rel="stylesheet" media="all and (max-width:767px)">
  {%- render 'preload-bottom-mobile-toolbar.css' with left_holder: left_holder, right_holder: right_holder -%}
</style>
<style rel="stylesheet" media="all and (min-width:992px)">
{%- case settings.header_style -%}
  {%- when 'style1' -%}
    {%- render 'preload-header-style1.css' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style2' -%}
    {%- render 'preload-header-style2.css' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style3' -%}
    {%- render 'preload-header-style3.css' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style4' -%}
    {%- render 'preload-header-style4.css' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style5' -%}
    {%- render 'preload-header-style5.css' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style6' -%}
    {%- render 'preload-header-style6.css' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style7' -%}
    {%- render 'preload-header-style7.css' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style8' -%}
    {%- render 'preload-header-style8.css' with left_holder: left_holder, right_holder: right_holder -%}
  {%- when 'style9' -%}
    {%- render 'preload-header-style9.css' with left_holder: left_holder, right_holder: right_holder -%}
  {%- else -%}
    {%- render 'preload-header-style10.css' with left_holder: left_holder, right_holder: right_holder -%}
{%- endcase -%}
.aligning-menu{overflow-x:hidden;}
</style>