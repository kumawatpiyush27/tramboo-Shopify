<div class="container{% render 'section-gutters-nobkg' %}">
  {%- for block in section.blocks -%}
    {% render block %}
  {%- endfor -%}
</div>
{% schema %}
{
  "name": "Apps",
  "tag": "section",
  "max_blocks": 12,
  "settings": [
    {
      "type": "checkbox",
      "id": "top_gutter",
      "label": "Enable top gutter",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "bottom_gutter",
      "label": "Enable bottom gutter",
      "default": false
    }
  ],
  "blocks": [
    {
      "type": "@app"
    }
  ],
  "presets": [
    {
      "name": "Apps"
    }
  ]
}
{% endschema %}