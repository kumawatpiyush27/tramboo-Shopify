<div class="container{% render 'section-gutters-nobkg' %}">
  <h1 class="page-title text-center gutter-ele-bottom-tbs">{{ page.title }}</h1>
  <div class="page-content w100">
    <div class="rte">
      {{ page.content }}
    </div>
  </div>
</div>
{% schema %}
{
  "name": "Page",
  "tag": "section",
  "settings": [
    {
      "type": "checkbox",
      "id": "top_gutter",
      "label": "Enable top gutter",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "bottom_gutter",
      "label": "Enable bottom gutter",
      "default": false
    }
  ]
}
{% endschema %}